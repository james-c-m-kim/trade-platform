<?xml version="1.0" encoding="utf-8"?>

<configuration>
  <appSettings>
    
    <add key="tickerWatchServer" value="tcp://localhost:4444" />
    
    <add key="pubSubServer" value="tcp://localhost:5555" />
    
    <add key="snapShotServer" value="tcp://localhost:3333" />
    
    <add key="fleckHost" value="localhost" />
    <add key="fleckPort" value="2222" />
